<h1>New Fileupload</h1>

<%= form_tag '/fileuploads', multipart: true  do %>
    <div class="field">
      <%= label_tag :project_name %><br>
      <%= text_field_tag :project_name %>
    </div>
    <div class="field">
      <%= label_tag :file_name %><br>
      <%= text_field_tag :file_name %>
    </div>
    <div class="field">
      <%= label_tag :start %><br>
      <%= text_field_tag :start %>
    </div>
    <div class="field">
      <%= label_tag :end %><br>
      <%= text_field_tag :end %>
    </div>
    <div class="field">
      <%= label_tag :unique %><br>
      <%= text_field_tag :unique %>
    </div>
    <div class="field">
      <%= label_tag :last_row %><br>
      <%= text_field_tag :last_row %>
    </div>
    <div>
      <%=file_field_tag :excel_file%>
    </div>
    <br/>
    <div class="actions">
      <%= submit_tag "import"%>
    </div>
<% end %>
